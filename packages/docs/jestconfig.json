{
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
  "automock": false,
  "testEnvironment": "node",
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
    "testPathIgnorePatterns": ["/lib/", "/node_modules/"],
    "collectCoverage": true
  }
